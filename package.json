{
  "name": "fxtch",
  "version": "0.1.0",
  "description": "A tiny wrapper around fetch for an easier API",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "check-ts": "tsc --project tsconfig.json",
    "build": "npm run check-ts && esbuild src/index.ts --minify --bundle --outfile=index.js",
    "test": "jest",
    "test:watch": "jest --watch --coverage=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheusbn/fxtch.git"
  },
  "keywords": [
    "fetch",
    "request",
    "superagent"
  ],
  "author": "matheusbnedel@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matheusbn/fxtch/issues"
  },
  "homepage": "https://github.com/matheusbn/fxtch#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "esbuild": "^0.13.3",
    "jest": "^27.2.4",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  }
}
